{
  "name": "leseq",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "description": "Lazy evaluation list with high tree-shaking affinity and easy customization.",
  "sideEffects": false,
  "author": "Masanori Onoue <ugaya40@hotmail.com> (https://github.com/ugaya40)",
